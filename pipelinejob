pipeline {
    agent any
    stages {
        stage('Compile Stage') { 
        
            steps {
                sh 'mvn clean'
            }
        }
        stage('Test') { 
        
           steps {
                sh 'test'
            }
        }
        stage('Deploy') { 
        
             steps {
                sh 'deploy'
            }
        }
    }
}
