pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                withMaven(maven : 'maven_3_8_4')
                sh 'mvn clean'
            }
        }
        stage('Test') { 
           steps {
                withMaven(maven : 'maven_3_8_4')
                sh 'mvn test'
            }
        }
        stage('Deploy') { 
             steps {
                withMaven(maven : 'maven_3_8_4')
                sh 'mvn deploy'
            }
        }
    }
}C 
